<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
	  <PackageId>Vdrio.Security.Encryption</PackageId>
	  <Version>0.9.0</Version>
	  <Authors>Lucas Glass</Authors>
	  <Company>Vdrio Inc</Company>
	  <Description>This library comes with class EncryptionManager that has easy-to-use Encrypt and Decrypt methods for securing your data.
	</Description>
	  <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
	  <RootNamespace>Vdrio.Security.Encryption</RootNamespace>
	  <LicenseUrl>https://github.com/Vdrio/Encryption/License.txt</LicenseUrl>
	  <License>MIT</License>
	  <Summary>.NET Standard AES Encryption</Summary>
	  <ProjectUrl>https://github.com/Vdrio/Encryption</ProjectUrl>
	  <RepositoryUrl>https://github.com/Vdrio/Encryption</RepositoryUrl>
	  <RepositoryType>Github</RepositoryType>
	  <PackageLicenseFile>License.txt</PackageLicenseFile>
	  <PackageProjectUrl>https://github.com/Vdrio/Encryption</PackageProjectUrl>
	  <Copyright>2020 Vdrio Inc</Copyright>
	  <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="License.txt" />
    <None Remove="License.txt" />
    <None Include="License.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
